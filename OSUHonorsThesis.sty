%-------------------------------------------------------------------------------------
%	TITLE PAGE
%-------------------------------------------------------------------------------------
\newcommand*{\ThesisTitlePage}{\begingroup
\newpage
\thispagestyle{empty}

\begin{singlespace}
  \begin{centering}

    \ThesisTitle \\[.05\textheight]
    {By \\[.02\textheight]  \StudentName}\\[0.167\textheight] 

    A DISSERTATION \\[.04\textheight]
    submitted to \\[.02\textheight]
    Oregon State University \\[.02\textheight]
    College of Science\\[.2\textheight]

    in partial fulfillment of\\
    the requirements for the\\
    degree of\\[.05\textheight]

	\ifdefined\SecondDegree
    	 of \Degree\ in \Major \\
        %(Honors \HonorsType)\\[.02\textheight]

    abc	Honors Baccalaureate of \SecondDegree\ in \SecondMajor\\
    %	(Honors \HonorsType)
    \else
    	\ifdefined\SecondMajor
           def   Honors Baccalaureate of \Degree\ in \Major\ and \SecondMajor\\
     %         (Honors \HonorsType)
        \else
         \Degree\ in \Major\\
      %      (Honors \HonorsType)
        \fi
   \fi

    \vfill

    Presented \DatePresented \\
    Commencement \CommencementMonth \\
  \end{centering}
\end{singlespace}
\newpage
\endgroup}

